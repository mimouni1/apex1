<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.LogInController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Login">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <Label layoutX="13.0" layoutY="41.0" text="Username:" />
        <TextField fx:id="usernameField" layoutX="81.0" layoutY="37.0" prefWidth="200.0" promptText="Enter username" />
        <Label layoutX="15.0" layoutY="79.0" text="Password:" />
        <PasswordField fx:id="passwordField" layoutX="81.0" layoutY="75.0" prefWidth="200.0" promptText="Enter password" />
       <Button fx:id="LogIn" layoutX="158.0" layoutY="115.0" mnemonicParsing="false" onAction="#LogInAction" prefHeight="25.0" prefWidth="122.0" text="Log In" />
<Button fx:id="SignUp" layoutX="25.0" layoutY="155.0" mnemonicParsing="false" onAction="#SignUpAction" prefHeight="25.0" prefWidth="74.0" text="Sign Up" />

      <Text layoutX="17.0" layoutY="145.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create an account ?" />
    </children>
</AnchorPane>
